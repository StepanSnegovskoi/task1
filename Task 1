import java.util.Scanner;
public class Main{

    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);
        System.out.println("Введите начальную скорость, м/с");
        double speed = scanner.nextDouble();
        final double g = 9.81;
        double result = func(speed, g);
        System.out.println(result + " секунд/ы");
    }

    public static double func (double speed, double g) {
        double formula = 2 * speed / g;
        return formula;

    }
}
